{
  "name": "app-server",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "server:build": "tsc -p tsconfig.build.json",
    "build": "rimraf dist && npm run server:build && npm run client:build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "server:test": "ts-node node_modules/jasmine/bin/jasmine ./src/**/*.spec.ts",
    "ng": "cd client && ng",
    "client:start": "cd client && ng serve --proxy-config ./proxy.config.json",
    "client:test": "cd client && ng test",
    "client:lint": "cd client && ng lint",
    "client:e2e": "cd client && ng e2e",
    "client:build": "cd client && ng run app-client:server:production && ng run app-client:build:production"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "^7.2.6",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "^7.2.6",
    "@angular/router": "~7.2.0",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/ng-universal": "^0.3.0",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "core-js": "^2.5.4",
    "dotenv": "^6.2.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "tslib": "^1.9.0",
    "typescript": "~3.2.2",
    "webpack-cli": "^3.2.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@nestjs/testing": "^5.1.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "codelyzer": "~4.5.0",
    "jasmine": "^3.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "protractor": "~5.4.0",
    "supertest": "^3.1.0",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "~5.11.0"
  }
}
